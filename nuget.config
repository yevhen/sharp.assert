<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- Local development feed takes priority -->
    <add key="local-dev" value="./local-feed" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  
  <!-- Package source mapping ensures SharpAssert packages come from local-dev -->
  <packageSourceMapping>
    <packageSource key="local-dev">
      <package pattern="SharpAssert*" />
    </packageSource>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>